<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Warranty Navigation Menu</masterLabel>
    <description>Custom Navigation Menu</description>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property
                name="menuName"
                type="String"
                label="Navigation Menu Name"
                description="The name of the Navigation Menu Linkset, which can be configured in Settings > Navigation"
            />
            <property type="ContentReference" name="contentId" label="Content ID" filter="cms_image"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>